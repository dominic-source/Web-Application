<section class="bgdark my-3">
  <div class="row">
    <div class="col-lg-4 col-md-6 col-sm-12 shape text-muted">
      <h1 class="fotfont">ABOUT US</h1>
      <p class="changefont2">
        cadatech is a fast developing company, hoping to bring her customers dream to life. </p>
      <div>
        <span><a href="#" class="cont"><i class="fab fa-facebook-f"></i></a></span>
        <span><a href="#" class="cont"><i class="fab fa-linkedin-in"></i></a></span>
        <span><a href="#" class="cont"><i class="fab fa-instagram"></i></a></span>
        <span><a href="#" class="cont"><i class="fab fa-twitter"></i></a></span>
      </div>


    </div>

    <div class="col-lg-4 col-md-6 col-sm-12 shape text-muted">
      <h1 class="fotfont">RECENT POST</h1>
      <div class="leftrecent ">
        <% blogPost.forEach(function(item,index){ if(index === blogPost.length-1 ||index === blogPost.length-2||index === blogPost.length-3){ %>
        <p class="changefont2">
          <a href="/myBlog/message/<%= item._id %>" class="nolink">
            <span class="angle">
              <i class="fa fa-arrow-right"></i>
            </span>
            <%= item.title %>
          </a>
        </p>
        <hr>
        <%  }}); %>
      </div>

    </div>
    <div class="col-lg-4 col-md-6 col-sm-12 shape fotfont text-muted" id="contacts">
      <h1 class="fotfont">CONTACT US</h1>

      <div class="">
        <h5><i class="fas fa-map-marker-alt con"></i> Address:</h5>
        <p>Ondo state </p>
      </div>

      <div class="">
        <h5><i class="fa fa-phone-square con" aria-hidden="true"></i>
          Phone:</h5>
        <p>08066246494</p>
      </div>

      <div class="">
        <h5><i class="fas fa-envelope con"></i> Mail:</h5>
        <p>chinonsodomnic@gmail.com</p>
      </div>
    </div>
    <footer class="my-5 pt-5 text-muted text-center text-small shapesmall">
      <p class="mb-1">&copy; 2020 till date cadatech</p>
      <ul class="list-inline">
        <li class="list-inline-item "><a href="#">Privacy</a></li>
        <li class="list-inline-item"><a href="#">Terms</a></li>
        <li class="list-inline-item"><a href="#">Support</a></li>
        <li><a href="/login" class="btn btn-primary">login</a></li>
      </ul>

    </footer>
</section>

