<section class="worksection " id="news">
  <h1 class="reasons">Our News</h1>

  <!-- bootstrap media -->
  <ul class="list-unstyled res">
    <% blogPost.forEach(function(item) {  %>

    <li class="media my-4 mx-1" style="background-color:aliceblue; border-radius:10px;">
      <img src="<%- item.imageurl %>" class="mr-2 newsimage" alt="...">
      <div class="media-body mr-2" >
        <h5 class="mt-0 mb-1 fonter"><%- item.title%></h5>
        <div class="OR"> <%- item.content.substr(0,30)%>&nbsp;...
          <!-- <div class=""> -->
            <a href="/myBlog/#<%= item._id %>" class="disp">VIEW DETAILS <span>></span></a>
            <small> <%=item.time%> : <%= item.authorName %> </small>
          <!-- </div> -->
        </div>
      </div>
    </li>

    <% });%>


  </ul>

  <!-- End of bootstrap media -->
</section>


